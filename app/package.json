{
  "name": "events-app",
  "version": "0.1.0",
  "private": true,
  "imports": {
    "@/*": "./*"
  },
  "scripts": {
    "db:reset": "bash scripts/reset-db.sh",
    "dev": "next dev",
    "lint": "next lint",
    "build": "next build",
    "build:test": "npm run db:reset& set NODE_ENV=test& next build",
    "start": "next start",
    "start:test": "set NODE_ENV=test& next start",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "cypress:open": "env-cmd -f .env.test cypress open",
    "cypress:start": "start-server-and-test 'npm run start:test' 3000 'npm run cypress:open'",
    "cypress:build": "npm run build:test && npm run cypress:start",
    "cypress:run": "start-server-and-test 'npm run start:test' 3000 'env-cmd -f .env.test cypress run'"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.3",
    "next": "^12.3.1",
    "next-i18next": "^12.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-toastify": "^9.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.2",
    "@types/node": "18.0.6",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "autoprefixer": "^10.4.12",
    "cypress": "10.3.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.24.0",
    "eslint-config-next": "^12.3.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-testing-library": "^5.7.2",
    "flag-icons": "^6.6.6",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "msw": "^0.47.4",
    "next-test-api-route-handler": "^3.1.7",
    "postcss": "^8.4.17",
    "start-server-and-test": "1.14.0",
    "tailwindcss": "^3.1.8",
    "typescript": "4.7.4",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
}
